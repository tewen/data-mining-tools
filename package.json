{
  "name": "data-mining-tools",
  "version": "2.0.0",
  "description": "Data mining helpers and utilities for Node JS.",
  "author": "Trevor Ewen",
  "license": "MIT",
  "module": "dist/data-mining-tools.esm.js",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "homepage": "https://github.com/tewen/data-mining-tools#readme",
  "bugs": {
    "url": "https://github.com/tewen/data-mining-tools/issues"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "test-watch": "tsdx test --watch",
    "lint": "tsdx lint",
    "lint-fix": "tsdx lint --fix",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "size-limit": [
    {
      "path": "dist/data-mining-tools.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/data-mining-tools.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "cheerio": "^1.0.0-rc.10",
    "deep-cuts": "^2.4.0",
    "diacritic": "^0.0.2",
    "fast-csv": "^4.3.2",
    "fs-extra": "^10.0.0",
    "parse-full-name": "^1.2.5"
  },
  "peerDependencies": {
    "axios": "^0.21.1"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^5.0.1",
    "@types/fs-extra": "^9.0.12",
    "axios": "^0.21.1",
    "husky": "^7.0.1",
    "size-limit": "^5.0.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5"
  }
}
